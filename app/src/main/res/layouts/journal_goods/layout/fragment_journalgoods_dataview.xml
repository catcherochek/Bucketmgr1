<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_collapseParallaxMultiplier="1.0">

    <TextView
        android:id="@+id/fragment_journalgoods_dataview_goodsarticul"
        android:layout_width="119dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/fragment_journalgoods_dataview_suppliername"
        android:textAppearance="?attr/textAppearanceListItem" />

    <TextView
        android:id="@+id/fragment_journalgoods_dataview_id"
        android:layout_width="25dp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/fragment_journalgoods_dataview_suppliername"
        android:layout_width="89dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textAppearance="?attr/textAppearanceListItem" />


    <TextView
        android:id="@+id/fragment_journalgoods_dataview_goodscount"
        android:layout_width="45dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/fragment_journalgoods_dataview_goodsarticul"
        android:textAppearance="?attr/textAppearanceListItem" />

    <TextView
        android:id="@+id/fragment_journalgoods_dataview_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>